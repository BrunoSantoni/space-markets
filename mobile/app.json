{
  "expo": {
    "name": "Space Markets",
    "slug": "spacemarkets",
    "version": "1.0.0",
    "githubUrl": "https://github.com/BrunoSantoni/space-markets",
    "orientation": "portrait",
    "primaryColor": "#63B1B9",
    "icon": "./assets/img/app-icon.png",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.spacemarkets",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "light-content",
      "backgroundColor": "#63B1B9"
    },
    "androidStatusBar": {
      "backgroundColor": "#00000000",
      "hidden": false,
      "translucent": true
    },
    "splash": {
      "backgroundColor": "#63B1B9",
      "image": "./assets/img/app-splash-icon.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic"
    }
  }
}
